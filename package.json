{
  "name": "docage",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "keywords": [
    "docage",
    "lowcode",
    "openapi"
  ],
  "scripts": {
    "install": "lerna bootstrap",
    "lint": "eslint --ext .ts packages/*/src/**.ts",
    "test": "npm run lint && lerna run test",
    "build": "lerna run build",
    "publish": "lerna publish && git commit -am \"Update version and changelog\" && git push origin master",
    "clean": "lerna clean",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "standard-version",
    "preinstall": "node ./scripts/checkYarn.js",
    "build:docs": "lerna run build"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@microsoft/api-extractor": "^7.18.4",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-url": "^6.1.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.3",
    "@types/puppeteer": "^5.4.4",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "@vue/compiler-sfc": "^3.2.4",
    "brotli": "^1.3.2",
    "chalk": "^4.1.1",
    "commitizen": "^4.2.1",
    "conventional-changelog-cli": "^2.1.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.12.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.16.0",
    "execa": "^5.1.1",
    "fs-extra": "^9.0.1",
    "husky": "^4.3.0",
    "lerna": "^3.22.1",
    "lint-staged": "^10.2.10",
    "minimist": "^1.2.5",
    "prettier": "^2.1.2",
    "puppeteer": "^10.1.0",
    "rollup": "^2.54.0",
    "rollup-plugin-polyfill-node": "^0.7.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "standard-version": "^9.0.0",
    "ts-jest": "^27.0.4",
    "typescript": "^4.0.5",
    "vue-eslint-parser": "^7.10.0",
    "zlib": "^1.0.5"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "packages/**/*.{ts,json}": [
      "prettier --ignore-path ./.prettierignore --write"
    ]
  },
  "author": "rayl <rayl@pisaas.com>",
  "license": "MIT",
  "homepage": "https://github.com/pisaas/docage#readme",
  "bugs": {
    "url": "https://github.com/pisaas/docage/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pisaas/docage.git"
  },
  "dependencies": {}
}
